{
  "manifest_version": 2,
  "name": "Truman's Weather Extension",
  "description": "This extension will tell you the weather.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "img/sun.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "activeTab"
  ]
}
